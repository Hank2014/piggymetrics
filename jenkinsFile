pipeline {
    agent any
    
    tools {
        // Install the Maven version configured as "M3" and add it to the path.
        maven "M3"
        jdk "MyJDK17"
    }
    
    stages {
        stage('checkout'){
            steps{
                checkout scm
            }
        }
        
        stage('readSCM'){
            steps{
                script{
                    def dCompose = readFile('docker-compose.yml')
                    def dComposeDev = readFile('docker-compose.dev.yml')
                    echo "Content of yml: \n ${dCompose}"
                }
            }
        }
        /* stage('SCM') {
